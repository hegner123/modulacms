{
  "name": "@modulacms/admin-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the ModulaCMS admin API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "tsup --clean"
  },
  "keywords": [
    "cms",
    "modulacms",
    "sdk",
    "typescript",
    "admin"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modulacms/types": "workspace:*"
  }
}
